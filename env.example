# Configuración de GenPassw Pro
# Copia este archivo como .env y configura tus valores

# Clave secreta para Flask (genera una clave segura)
SECRET_KEY=tu-clave-secreta-super-segura-aqui

# Configuración del entorno
FLASK_ENV=development
FLASK_DEBUG=True

# Configuración del servidor
HOST=0.0.0.0
PORT=5000

# Configuración de la base de datos (opcional)
DATABASE_URL=sqlite:///passwords.db

# Configuración de seguridad
SESSION_COOKIE_SECURE=False
SESSION_COOKIE_HTTPONLY=True
